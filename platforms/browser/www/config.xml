<?xml version='1.0' encoding='utf-8'?>
<widget id="com.emin.VKindex" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>VKindex</name>
    <description>
    Vücut kitle indeks hesaplama uygulaması
    </description>
    <author email="pinoki-tr@hotmail.com" href="http://eminkose.com">
       M. Emin KÖSE
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
